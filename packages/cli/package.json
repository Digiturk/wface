	"scripts": {
		"build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
		"build-dev": "yarn run clean-dist && webpack --config=configs/webpack/prod.js --mode=development",
		"clean-dist": "rimraf dist",
		"start": "yarn run start-dev",
		"start-dev": "webpack-dev-server --config=configs/webpack/dev.js --open",
		"start-prod": "yarn run build && node express.js",
		"docker": "npm run build && docker build -t wface .",
		"start:docker": "docker run -p 81:80 digiturk/wface"
	},
